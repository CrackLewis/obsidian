{
  "database": "Zotero",
  "noteImportFolder": "Zotero/Imports",
  "pdfExportImageDPI": 120,
  "pdfExportImageFormat": "jpg",
  "pdfExportImageQuality": 90,
  "citeFormats": [
    {
      "name": "Cite",
      "format": "formatted-citation",
      "cslStyle": "china-national-standard-gb-t-7714-2015-numeric"
    }
  ],
  "exportFormats": [
    {
      "name": "Import #1",
      "outputPathTemplate": "Zotero/Notes/{{citekey}}.md",
      "imageOutputPathTemplate": "Zotero/Images/{{citekey}}/",
      "imageBaseNameTemplate": "image",
      "templatePath": "Zotero/template.md",
      "cslStyle": "china-national-standard-gb-t-7714-2015-author-date"
    }
  ],
  "citeSuggestTemplate": "[[{{citekey}}]]",
  "openNoteAfterImport": false,
  "whichNotesToOpenAfterImport": "first-imported-note"
}